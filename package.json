{
  "name": "codify",
  "displayName": "Codify",
  "description": "",
  "publisher": "Small Cloud Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/smallcloudai/plugin-vscode"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Codify",
      "properties": {
        "mate.model": {
          "type": "string",
          "default": "CONTRASTcode/large/stable",
          "enum": [
            "CONTRASTcode/large/debug",
            "CONTRASTcode/large/stable"
          ],
          "description": "Model Name"
        }
      }
    },
    "commands": [
      {
        "command": "plugin-vscode.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "plugin-vscode.menu0",
        "title": "Quick Input",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.menu1",
        "title": "Highlight",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.menu2",
        "title": "Menu 2",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.fixTypes",
        "title": "Fix types",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.fixUnclearNames",
        "title": "Fix unclear names",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.variablesShorter",
        "title": "Make variables shorter",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.codeShorter",
        "title": "Make code shorter",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.improvePerformance",
        "title": "Improve performance",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.codeCleanup",
        "title": "Code cleanup",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.formattingConsistent",
        "title": "Make formatting consistent",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.removePython2",
        "title": "Remove python 2 support",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.convertNumpy",
        "title": "Convert to numpy",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.convertDictClass",
        "title": "Convert dict to class",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.listComprehension",
        "title": "Convert to list comprehension",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.addDocstrings",
        "title": "Add docstrings",
        "category": "Codify"
      },
      {
        "command": "plugin-vscode.fixTypos",
        "title": "Fix typos",
        "category": "Codify"
      },
      {
        "command": "extension.addConsoleLog",
        "title": "Add Console Log"
      }
    ],
    "properties": [
      {
        "id": "plugin-vscode.contrastUrl",
        "title": "Use CONTRAST model at this URL",
        "type": "string",
        "default": "https://inference.smallcloud.ai/v1/contrast"
      }
    ],
    "keybindings": [
      {
        "command": "plugin-vscode.f1",
        "key": "F1"
      },
      {
        "command": "plugin-vscode.esc",
        "key": "escape",
        "when" : "codify.runEsc"
      },
      {
        "command": "plugin-vscode.tab",
        "key": "tab",
        "when" : "codify.runTab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "fetch-h2": "2",
    "diff": "^5.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "diff": "^5.0.0",
    "eslint": "^8.18.0",
    "fetch-h2": "2",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
