{
  "name": "vscode-mate",
  "displayName": "vscode-mate",
  "description": "",
  "publisher": "Small Cloud Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/smallcloudai/plugin-vscode"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "VSCode Mate",
      "properties": {
        "mate.model": {
          "type": "string",
          "default": "CONTRASTcode/large/v5",
          "enum": [
            "CONTRASTcode/large",
            "CONTRASTcode/large/v5"
          ],
          "description": "Model Name"
        }
      }
    },
    "commands": [
      {
        "command": "plugin-vscode.helloWorld",
        "title": "Hello World"
      }
    ],
    "properties": [
      {
        "id": "plugin-vscode.contrastUrl",
        "title": "Use CONTRAST model at this URL",
        "type": "string",
        "default": "https://inference.smallcloud.ai/v1/contrast"
      }
    ],
    "keybindings": [
      {
        "command": "plugin-vscode.f1",
        "key": "F1"
      },
      {
        "command": "plugin-vscode.esc",
        "key": "escape",
        "when" : "vscode-mate.runEsc"
      },
      {
        "command": "plugin-vscode.tab",
        "key": "tab",
        "when" : "vscode-mate.runTab"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "fetch-h2": "2",
    "diff": "^5.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.69.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vscode/test-electron": "^2.1.5",
    "diff": "^5.0.0",
    "eslint": "^8.18.0",
    "fetch-h2": "2",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  }
}
